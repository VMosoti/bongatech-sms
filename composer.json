{
  "name": "vmosoti/bongatech-sms",
  "description": "A PHP client library for BongaTech's API (http://bongatech.co.ke)",
  "keywords": [
    "sms",
    "gateway",
    "bongatech"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vincent Mosoti",
      "email": "vincent@vmosoti.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.4",
    "guzzlehttp/guzzle": "~6.0",
    "tightenco/collect": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "files": [
      "src/bongatech-sms/helpers.php"
    ],
    "psr-4": {
      "VMosoti\\BongaTech\\": "src/bongatech-sms"
    }
  }
}
